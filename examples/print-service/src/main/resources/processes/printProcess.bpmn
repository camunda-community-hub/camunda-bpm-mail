    </bpmn:textAnnotation>
    <bpmn:association id="Association_07i4zlv" sourceRef="ServiceTask_1ry54cw" targetRef="TextAnnotation_108qpjm" />
    <bpmn:textAnnotation id="TextAnnotation_0v0w0hp">
